//asus_bsp_mike ++

&soc {
	//ASUS_BSP_Mike++
	camera_rear0_vana_ldo: gpio-regulator@3 {//something wrong?
		compatible = "regulator-fixed";
		reg = <0x03 0x00>;//something wrong?
		regulator-name = "camera_rear0_vana_ldo";//mike change to rear0
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		regulator-enable-ramp-delay = <233>;
		enable-active-high;
		gpio = <&pm8998_gpios 13 0>;//mike 
		pinctrl-names = "default";
		pinctrl-0 = <&camera_rear0_vana_en_default>;//mike change to 0
		vin-supply = <&pmi8998_bob>;
	};
	//ASUS_BSP_Mike++

	camera_rear1_vdig_ldo: gpio-regulator@4 {
		compatible = "regulator-fixed";
		reg = <0x04 0x00>;
		regulator-name = "camera_rear1_vdig_ldo";
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1200000>;
		regulator-enable-ramp-delay = <233>;
		enable-active-high;
		gpio = <&pm8998_gpios 15 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&camera_rear1_vdig_en_default>;
		vin-supply = <&pm8998_s4>;
	};

	camera_rear_vdig_ldo: gpio-regulator@5 {
		compatible = "regulator-fixed";
		reg = <0x05 0x00>;
		regulator-name = "camera_rear_vdig_ldo";
		regulator-min-microvolt = <1050000>;
		regulator-max-microvolt = <1050000>;
		regulator-enable-ramp-delay = <233>;
		enable-active-high;
		gpio = <&pm8998_gpios 14 0>; 
		pinctrl-names = "default";
		pinctrl-0 = <&camera_rear_vdig_en_default>;
		vin-supply = <&pmi8998_bob>;
	};

	camera_rear_vana_ldo: gpio-regulator@6 {
		compatible = "regulator-fixed";
		reg = <0x06 0x00>;
		regulator-name = "camera_rear_vana_ldo";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		regulator-enable-ramp-delay = <233>;
		enable-active-high;
		gpio = <&pm8998_gpios 5 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&camera_rear_vana_en_default>;
		vin-supply = <&pmi8998_bob>;
	};	

	//ASUS_BSP_Mike +++
	camera_rearimx363_vdig_ldo: gpio-regulator@7 {
		compatible = "regulator-fixed";
		reg = <0x07 0x00>;
		regulator-name = "camera_rearimx363_vdig_ldo";
		regulator-min-microvolt = <1050000>;
		regulator-max-microvolt = <1050000>;
		regulator-enable-ramp-delay = <233>;
		enable-active-high;
		gpio = <&pmi8998_gpios 14 0>; //add i
		pinctrl-names = "default";
		pinctrl-0 = <&camera_rearimx363_vdig_en_default>;//mike change
		vin-supply = <&pmi8998_bob>;
	};

	//ASUS_BSP_Mike +++

};
//ASUS_BSP flash +++
&pmi8998_flash0 {
	qcom,max-current = <1000>;
	qcom,current-ma = <600>;
};

&pmi8998_flash1 {
	status = "disabled";
};

&pmi8998_flash2 {
	status = "disabled";
};

&pmi8998_torch0 {
	qcom,max-current = <200>;
	qcom,current-ma = <200>;

};

&pmi8998_torch1 {
	status = "disabled";
};

&pmi8998_torch2 {
	status = "disabled";
};

&pmi8998_switch1 {
	status = "disabled";
};

&led_flash_rear {
	flash-source = <&pmi8998_flash0>;
	torch-source = <&pmi8998_torch0>;
};

&led_flash_rear_aux {
	flash-source = <&pmi8998_flash0>;
	torch-source = <&pmi8998_torch0>;
};

&led_flash_front {
	status = "disabled";
};
&led_flash_iris {
	status = "disabled";
};

//ASUS_BSP flash ---
//ASUS_BSP CCI +++
&i2c_freq_100Khz {
	hw-scl-stretch-en = <1>;
};
&i2c_freq_400Khz {
	hw-scl-stretch-en = <1>;
};
&i2c_freq_1Mhz {
	hw-scl-stretch-en = <1>;
};
//ASUS_BSP CCI ---
//ASUS_BSP VCM +++
&actuator_regulator {
	gpio = <&pm8998_gpios 5 0>; //mike change to 5 original (13)
	pinctrl-names = "cam_default", "cam_suspend";//mike
	pinctrl-1 = <&camera_vaf_en_default>;//mike
};

&cam_cci {
	actuator_rear1: qcom,actuator@72 {
		cell-index = <0>;
		reg = <0x72>;
		compatible = "qcom,actuator";
		cci-master = <0>;
		qcom,i2c-freq-mode = <1>;
		qcom,slave-id = <0x72 0xF0 0x42>;
		status = "ok";
	};
};

&actuator_rear {
	status = "disabled";
};

&actuator_rear_aux {
	status = "disabled";
};

&actuator_front {
	status = "disabled";
};
//ASUS_BSP VCM ---

//ASUS_BSP OIS +++
&ois_rear {
	status = "disabled";
};

&cam_cci {
	ois_rear1: qcom,ois@3E {
		cell-index = <0>;
		reg = <0x3E>;
		compatible = "qcom,lc898123f40";
		cci-master = <0>;
		qcom,i2c-freq-mode = <1>;
		qcom,slave-id = <0x7C 0xD00100 0x00000082>;

		rgltr-cntrl-support;
		cam_vaf-supply = <&actuator_regulator>;
		cam_vana-supply = <&pm8998_l21>;
		regulator-names = "cam_vaf", "cam_vana";
		rgltr-min-voltage = <2800000 2800000>;
		rgltr-max-voltage = <2800000 2800000>;
		rgltr-load-current = <300000 500000>;


		qcom,cam-power-seq-type = "cam_vaf","cam_vana";
		qcom,cam-power-seq-cfg-val = <2800000 2800000>;
		qcom,cam-power-seq-delay = <0 100>;//delay 1ms for VIO, delay 100ms for OIS

		status = "ok";
	};

};
//ASUS_BSP OIS ---

//ASUS_BSP EEPROM +++
&cam_cci {
	eeprom_rear0: qcom,eeprom@10 {
		cell-index = <0>;
		reg = <0x0>;
		cci-master = <0>;
		compatible = "qcom,eeprom";
		cam_vio-supply = <&pm8998_lvs1>;
		regulator-names = "cam_vio";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000>;
		rgltr-max-voltage = <1800000>;
		rgltr-load-current = <300000>;
		status = "ok";
	};
	eeprom_front1: qcom,eeprom@11 {
		cell-index = <1>;
		reg = <0x1>;
		cci-master = <0>;
		compatible = "qcom,eeprom";
		cam_vio-supply = <&pm8998_lvs1>;
		regulator-names = "cam_vio";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000>;
		rgltr-max-voltage = <1800000>;
		rgltr-load-current = <300000>;
		status = "ok";
	};
	eeprom_rear_aux2: qcom,eeprom@12 {
		cell-index = <2>;
		reg = <0x2>;
		cci-master = <0>;
		compatible = "qcom,eeprom";
		cam_vio-supply = <&pm8998_lvs1>;
		regulator-names = "cam_vio";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000>;
		rgltr-max-voltage = <1800000>;
		rgltr-load-current = <300000>;
		status = "ok";
	};
};


	

&eeprom_rear {
	status = "disabled";
};
&eeprom_rear_aux {
	status = "disabled";
};

&eeprom_front {
	status = "disabled";
};
//ASUS_BSP EEPROM ---

//ASUS_BSP sensor +++
&cam_cci {
	qcom,cam-sensor@0 {
		cell-index = <0>;
		compatible = "qcom,cam-sensor";
		reg = <0x0>;
		csiphy-sd-index = <0>;
		sensor-position-roll = <90>;
		sensor-position-pitch = <0>;
		sensor-position-yaw = <180>;
		led-flash-src = <&led_flash_rear>;
		actuator-src = <&actuator_rear1>;
		ois-src = <&ois_rear1>; //mike remove
		eeprom-src = <&eeprom_rear0>;
		cam_vio-supply = <&pm8998_lvs1>;
		cam_vana-supply = <&camera_rear0_vana_ldo>;//mike change to rear0
		cam_vdig-supply = <&camera_rearimx363_vdig_ldo>;//asus_bsp_mike +++
		//cam_vaf-supply = <&pm8998_l22>;
		cam_clk-supply = <&titan_top_gdsc>;
		regulator-names = "cam_vio", "cam_vana", "cam_vdig",
			"cam_vaf","cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 2800000 1050000 2864000 0>;
		rgltr-max-voltage = <1800000 2800000 1050000 2864000 0>;
		rgltr-load-current = <300000 100000 232300 0 0>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk1_active>;
		pinctrl-1 = <&cam_sensor_mclk1_suspend>;
		gpios = <&tlmm 14 0>,
			<&tlmm 69 0>;
		gpio-reset = <1>;
		gpio-vana = <0x0>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK0",
					"CAM_RESET0";
		sensor-mode = <0>;
		cci-master = <0>;
		status = "ok";
		clocks = <&clock_camcc CAM_CC_MCLK1_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "turbo";
		clock-rates = <19200000>;
	};

	qcom,cam-sensor@1 {
		cell-index = <1>;
		compatible = "qcom,cam-sensor";
		reg = <0x1>;
		csiphy-sd-index = <2>;
		sensor-position-roll = <270>;
		sensor-position-pitch = <0>;
		sensor-position-yaw = <0>;
		actuator-src = <0x0>;
		led-flash-src = <0x0>;
		eeprom-src = <&eeprom_front1>;
		cam_vdig-supply = <&camera_rear1_vdig_ldo>;
		cam_vio-supply = <&pm8998_lvs1>;
		cam_vana-supply = <&camera_rear_vana_ldo>;
		cam_clk-supply = <&titan_top_gdsc>;
		regulator-names = "cam_vdig","cam_vio", "cam_vana",
			"cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1200000 1800000 2800000 0>;
		rgltr-max-voltage = <1200000 1800000 2800000 0>;
		rgltr-load-current = <300000 300000 21500 0>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk2_active>;
		pinctrl-1 = <&cam_sensor_mclk2_suspend>;
		gpios = <&tlmm 15 0>,
			<&tlmm 9 0>;
		gpio-reset = <1>;
		gpio-vana = <0x0>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK1",
					"CAM_RESET1";
		sensor-mode = <0>;
		cci-master = <1>;
		status = "ok";
		clocks = <&clock_camcc CAM_CC_MCLK2_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "turbo";
		clock-rates = <6400000>;
	};

	qcom,cam-sensor@2 {
		cell-index = <2>;
		compatible = "qcom,cam-sensor";
		reg = <0x2>;
		csiphy-sd-index = <1>;
		sensor-position-roll = <90>;
		sensor-position-pitch = <0>;
		sensor-position-yaw = <180>;
		actuator-src = <0x0>;
		led-flash-src = <&led_flash_rear_aux>;
		eeprom-src = <&eeprom_rear_aux2>;
		cam_vdig-supply = <&camera_rear1_vdig_ldo>;
		cam_vio-supply = <&pm8998_lvs1>;
		cam_vana-supply = <&camera_rear_vana_ldo>;
		cam_clk-supply = <&titan_top_gdsc>;
		regulator-names = "cam_vdig","cam_vio", "cam_vana",
			"cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1200000 1800000 2800000 0>;
		rgltr-max-voltage = <1200000 1800000 2800000 0>;
		rgltr-load-current = <300000 300000 21500 0>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_active>;
		pinctrl-1 = <&cam_sensor_mclk0_suspend>;
		gpios = <&tlmm 13 0>,
			<&tlmm 16 0>;
		gpio-reset = <1>;
		gpio-vana = <0x0>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK2",
					"CAM_RESET2";
		sensor-mode = <0>;
		cci-master = <1>;
		status = "ok";
		clocks = <&clock_camcc CAM_CC_MCLK0_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "turbo";
		clock-rates = <6400000>;
	};

	qcom,cam-sensor@3 {
		status = "disabled";
	};
};
